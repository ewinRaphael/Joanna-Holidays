        <script>
            function showGalleryTab(tab) {
                const imagesTab = document.getElementById('imagesTab');
                const videosTab = document.getElementById('videosTab');
                const imagesBtn = document.getElementById('imagesTabBtn');
                const videosBtn = document.getElementById('videosTabBtn');
                if (tab === 'images') {
                    imagesTab.classList.remove('hidden');
                    videosTab.classList.add('hidden');
                    imagesBtn.classList.add('bg-red-500', 'text-white');
                    imagesBtn.classList.remove('bg-gray-800', 'hover:bg-gray-700');
                    videosBtn.classList.remove('bg-red-500', 'text-white');
                    videosBtn.classList.add('bg-gray-800', 'hover:bg-gray-700');
                } else {
                    imagesTab.classList.add('hidden');
                    videosTab.classList.remove('hidden');
                    videosBtn.classList.add('bg-red-500', 'text-white');
                    videosBtn.classList.remove('bg-gray-800', 'hover:bg-gray-700');
                    imagesBtn.classList.remove('bg-red-500', 'text-white');
                    imagesBtn.classList.add('bg-gray-800', 'hover:bg-gray-700');
                }
            }
            // On page load, show images tab by default
            document.addEventListener('DOMContentLoaded', function() {
                showGalleryTab('images');
            });
        </script>
